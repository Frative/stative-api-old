// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Summoner {
  id String @id @map("_id")
  region String
  summonerId String
  accountId String
  puuid String
  name String
  profileIconId Int
  revisionDate Int
  summonerLevel Int
}